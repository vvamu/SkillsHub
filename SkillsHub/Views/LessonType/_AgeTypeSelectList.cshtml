@model (List<SkillsHub.Domain.Models.AgeType>, Guid)
<
<select>
    @foreach(var item in Model.Item1)
    {
        var min = item.MinimumAge;
        var max = item.MaximumAge;


        if(item.Id == Model.Item2)
       {
           <option value="@item.Id" selected>@item.Name 
               @if(min == max) 
               {
                    <span>(@min years)</span>
               }
               else{
                    <span>(@min - @max years)</span>
               }
           </option>
       }
       else{
            <option value="@item.Id">@item.Name
                @if (min == max)
                {
                    <span>(@min years)</span>
                }
                else
                {
                    <span>(@min - @max years)</span>
                }
           </option>
       }
    }
    @if (User.IsInRole("Admin"))
    {
    <option id="last"><input type="button" value="create new" /></option>
    }

</select>


    <script>
        function addNewRow() { 
        
        }
    </script>
